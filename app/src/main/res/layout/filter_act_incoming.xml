<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:visibility="visible"
    android:id="@+id/cons_incoming_filter"
    android:layout_height="match_parent">
    <androidx.core.widget.NestedScrollView
        android:id="@+id/nested"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <androidx.appcompat.widget.LinearLayoutCompat
            android:orientation="vertical"
            android:layout_margin="@dimen/_10sdp"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
           <androidx.appcompat.widget.LinearLayoutCompat
               android:orientation="vertical"
               android:layout_width="match_parent"
               android:layout_height="wrap_content">
               <TextView
                   android:fontFamily="@font/inter_medium"
                   android:textColor="@color/text_color"
                   android:textStyle="bold"
                   android:textSize="@dimen/_10sdp"
                   android:text="@string/act"
                   android:layout_width="match_parent"
                   android:layout_height="wrap_content"/>
               <androidx.appcompat.widget.LinearLayoutCompat
                   android:layout_marginTop="@dimen/_8sdp"
                   android:orientation="horizontal"
                   android:layout_width="match_parent"
                   android:layout_height="wrap_content">
                   <androidx.appcompat.widget.LinearLayoutCompat
                       android:layout_weight="1"
                       android:layout_marginEnd="@dimen/_2sdp"
                       android:orientation="vertical"
                       android:layout_width="match_parent"
                       android:layout_height="wrap_content">
                       <TextView
                           android:textSize="@dimen/_8sdp"
                           android:textStyle="normal"
                           android:textColor="@color/text_color_gray"
                           android:text="@string/number"
                           android:layout_width="match_parent"
                           android:layout_height="wrap_content"/>
                       <EditText
                           android:id="@+id/doc_number"
                           android:layout_weight="1"
                           android:inputType="text"
                           android:layout_marginTop="@dimen/_4sdp"
                           android:paddingVertical="@dimen/_7sdp"
                           android:textColor="@color/text_color"
                           android:textStyle="normal"
                           android:textSize="@dimen/_10sdp"
                           android:paddingHorizontal="@dimen/_5sdp"
                           android:background="@drawable/create_edit_text_background"
                           android:layout_width="match_parent"
                           android:layout_height="wrap_content"/>
                   </androidx.appcompat.widget.LinearLayoutCompat>
                   <androidx.appcompat.widget.LinearLayoutCompat
                       android:id="@+id/linear_start_act_date"
                       android:layout_weight="1"
                       android:layout_marginStart="@dimen/_2sdp"
                       android:orientation="vertical"
                       android:layout_width="match_parent"
                       android:layout_height="wrap_content">
                       <TextView
                           android:textSize="@dimen/_8sdp"
                           android:textStyle="normal"
                           android:textColor="@color/text_color_gray"
                           android:text="@string/from_date"
                           android:layout_width="match_parent"
                           android:layout_height="wrap_content"/>
                       <TextView
                           android:id="@+id/date_act_start"
                           android:inputType="text"
                           android:layout_weight="1"
                           android:layout_marginTop="@dimen/_4sdp"
                           android:paddingVertical="@dimen/_7sdp"
                           android:textColor="@color/text_color"
                           android:textStyle="normal"
                           android:textSize="@dimen/_10sdp"
                           android:paddingHorizontal="@dimen/_5sdp"
                           android:background="@drawable/create_edit_text_background"
                           android:layout_width="match_parent"
                           android:layout_height="wrap_content"/>
                   </androidx.appcompat.widget.LinearLayoutCompat>
               </androidx.appcompat.widget.LinearLayoutCompat>
               <androidx.appcompat.widget.LinearLayoutCompat
                   android:layout_marginTop="@dimen/_8sdp"
                   android:orientation="horizontal"
                   android:layout_width="match_parent"
                   android:layout_height="wrap_content">
                   <androidx.appcompat.widget.LinearLayoutCompat
                       android:layout_weight="1"
                       android:layout_marginEnd="@dimen/_2sdp"
                       android:orientation="vertical"
                       android:layout_width="match_parent"
                       android:layout_height="wrap_content">
                       <TextView
                           android:id="@+id/date_end"
                           android:textSize="@dimen/_8sdp"
                           android:textStyle="normal"
                           android:textColor="@color/text_color_gray"
                           android:text="@string/date_by"
                           android:layout_width="match_parent"
                           android:layout_height="wrap_content"/>
                       <TextView
                           android:id="@+id/date_act_end"
                           android:layout_weight="1"
                           android:layout_marginTop="@dimen/_4sdp"
                           android:paddingVertical="@dimen/_7sdp"
                           android:textColor="@color/text_color"
                           android:textStyle="normal"
                           android:textSize="@dimen/_10sdp"
                           android:paddingHorizontal="@dimen/_5sdp"
                           android:background="@drawable/create_edit_text_background"
                           android:layout_width="match_parent"
                           android:layout_height="wrap_content"/>
                   </androidx.appcompat.widget.LinearLayoutCompat>
                   <androidx.appcompat.widget.LinearLayoutCompat
                       android:layout_weight="1"
                       android:layout_marginStart="@dimen/_2sdp"
                       android:orientation="vertical"
                       android:layout_width="match_parent"
                       android:layout_height="wrap_content">
                       <TextView
                           android:textSize="@dimen/_8sdp"
                           android:textStyle="normal"
                           android:textColor="@color/text_color_gray"
                           android:text="@string/status"
                           android:layout_width="match_parent"
                           android:layout_height="wrap_content"/>
                       <TextView
                           android:id="@+id/status_checked"
                           android:layout_weight="1"
                           android:layout_marginTop="@dimen/_4sdp"
                           android:paddingVertical="@dimen/_7sdp"
                           android:textColor="@color/text_color"
                           android:textStyle="normal"
                           android:textSize="@dimen/_10sdp"
                           android:paddingHorizontal="@dimen/_5sdp"
                           android:background="@drawable/create_edit_text_background"
                           android:layout_width="match_parent"
                           android:layout_height="wrap_content"/>
                   </androidx.appcompat.widget.LinearLayoutCompat>
               </androidx.appcompat.widget.LinearLayoutCompat>
           </androidx.appcompat.widget.LinearLayoutCompat>
            <androidx.appcompat.widget.LinearLayoutCompat
                android:layout_marginTop="@dimen/_8sdp"
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:fontFamily="@font/inter_medium"
                    android:textColor="@color/text_color"
                    android:textStyle="bold"
                    android:textSize="@dimen/_10sdp"
                    android:text="@string/contract"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"/>
                <androidx.appcompat.widget.LinearLayoutCompat
                    android:layout_marginTop="@dimen/_8sdp"
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <androidx.appcompat.widget.LinearLayoutCompat
                        android:layout_weight="1"
                        android:layout_marginEnd="@dimen/_2sdp"
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <TextView
                            android:textSize="@dimen/_8sdp"
                            android:textStyle="normal"
                            android:textColor="@color/text_color_gray"
                            android:text="@string/number"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"/>
                        <EditText
                            android:id="@+id/contract_number"
                            android:layout_weight="1"
                            android:inputType="text"
                            android:layout_marginTop="@dimen/_4sdp"
                            android:paddingVertical="@dimen/_7sdp"
                            android:textColor="@color/text_color"
                            android:textStyle="normal"
                            android:textSize="@dimen/_10sdp"
                            android:paddingHorizontal="@dimen/_5sdp"
                            android:background="@drawable/create_edit_text_background"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"/>
                    </androidx.appcompat.widget.LinearLayoutCompat>
                    <androidx.appcompat.widget.LinearLayoutCompat
                        android:layout_weight="1"
                        android:layout_marginStart="@dimen/_2sdp"
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <TextView
                            android:textSize="@dimen/_8sdp"
                            android:textStyle="normal"
                            android:textColor="@color/text_color_gray"
                            android:text="@string/from_date"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"/>
                        <TextView
                            android:id="@+id/date_start_contract"
                            android:layout_weight="1"
                            android:layout_marginTop="@dimen/_4sdp"
                            android:paddingVertical="@dimen/_7sdp"
                            android:textColor="@color/text_color"
                            android:textStyle="normal"
                            android:textSize="@dimen/_10sdp"
                            android:paddingHorizontal="@dimen/_5sdp"
                            android:background="@drawable/create_edit_text_background"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"/>
                    </androidx.appcompat.widget.LinearLayoutCompat>
                </androidx.appcompat.widget.LinearLayoutCompat>
                <androidx.appcompat.widget.LinearLayoutCompat
                    android:layout_marginTop="@dimen/_8sdp"
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <androidx.appcompat.widget.LinearLayoutCompat
                        android:layout_weight="1"
                        android:layout_marginEnd="@dimen/_2sdp"
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <TextView
                            android:id="@+id/date_end_contract"
                            android:textSize="@dimen/_8sdp"
                            android:textStyle="normal"
                            android:textColor="@color/text_color_gray"
                            android:text="@string/date_by"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"/>
                        <TextView
                            android:id="@+id/date_contract_end"
                            android:layout_weight="1"
                            android:layout_marginTop="@dimen/_4sdp"
                            android:paddingVertical="@dimen/_7sdp"
                            android:textColor="@color/text_color"
                            android:textStyle="normal"
                            android:textSize="@dimen/_10sdp"
                            android:paddingHorizontal="@dimen/_5sdp"
                            android:background="@drawable/create_edit_text_background"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"/>
                    </androidx.appcompat.widget.LinearLayoutCompat>
                </androidx.appcompat.widget.LinearLayoutCompat>
            </androidx.appcompat.widget.LinearLayoutCompat>
            <androidx.appcompat.widget.LinearLayoutCompat
                android:layout_marginTop="@dimen/_8sdp"
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:fontFamily="@font/inter_medium"
                    android:textColor="@color/text_color"
                    android:textStyle="bold"
                    android:textSize="@dimen/_10sdp"
                    android:text="@string/other"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"/>
                <androidx.appcompat.widget.LinearLayoutCompat
                    android:layout_marginTop="@dimen/_8sdp"
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <androidx.appcompat.widget.LinearLayoutCompat
                        android:layout_weight="1"
                        android:layout_marginEnd="@dimen/_2sdp"
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <TextView
                            android:textSize="@dimen/_8sdp"
                            android:textStyle="normal"
                            android:textColor="@color/text_color_gray"
                            android:text="@string/counterparty"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"/>
                        <EditText
                            android:id="@+id/inn"
                            android:inputType="number"
                            android:maxLength="9"
                            android:layout_weight="1"
                            android:layout_marginTop="@dimen/_4sdp"
                            android:paddingVertical="@dimen/_7sdp"
                            android:textColor="@color/text_color"
                            android:textStyle="normal"
                            android:textSize="@dimen/_10sdp"
                            android:paddingHorizontal="@dimen/_5sdp"
                            android:background="@drawable/create_edit_text_background"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"/>
                    </androidx.appcompat.widget.LinearLayoutCompat>
                </androidx.appcompat.widget.LinearLayoutCompat>
                <androidx.appcompat.widget.LinearLayoutCompat
                    android:layout_marginTop="@dimen/_8sdp"
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <androidx.appcompat.widget.LinearLayoutCompat
                        android:layout_weight="1"
                        android:layout_marginEnd="@dimen/_2sdp"
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <TextView
                            android:id="@+id/subdivisions_title"
                            android:textSize="@dimen/_8sdp"
                            android:textStyle="normal"
                            android:textColor="@color/text_color_gray"
                            android:text="@string/subdivisions"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"/>
                        <androidx.constraintlayout.widget.ConstraintLayout
                            android:layout_marginTop="@dimen/_4sdp"
                            android:background="@drawable/create_edit_text_background"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">
                            <androidx.appcompat.widget.LinearLayoutCompat
                                android:orientation="horizontal"
                                android:gravity="center"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent">
                                <TextView
                                    android:id="@+id/subdivisions_edit_tv"
                                    android:layout_weight="1"
                                    android:paddingVertical="@dimen/_7sdp"
                                    android:textColor="@color/text_color"
                                    android:textStyle="normal"
                                    android:textSize="@dimen/_10sdp"
                                    android:paddingHorizontal="@dimen/_5sdp"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"/>
                                <ImageView
                                    android:visibility="gone"
                                    android:id="@+id/cancel_branch_seller"
                                    android:src="@drawable/ic_baseline_cancel_24"
                                    android:layout_marginHorizontal="@dimen/_5sdp"
                                    android:padding="@dimen/_2sdp"
                                    android:layout_width="@dimen/_20sdp"
                                    android:layout_height="@dimen/_20sdp"/>
                            </androidx.appcompat.widget.LinearLayoutCompat>
                            <ProgressBar
                                android:id="@+id/progress_act_draft"
                                android:visibility="gone"
                                android:indeterminateTint="@color/secondary_color"
                                app:layout_constraintRight_toRightOf="parent"
                                app:layout_constraintTop_toTopOf="parent"
                                app:layout_constraintBottom_toBottomOf="parent"
                                android:layout_width="@dimen/_15sdp"
                                android:layout_height="@dimen/_15sdp"/>
                        </androidx.constraintlayout.widget.ConstraintLayout>
                    </androidx.appcompat.widget.LinearLayoutCompat>
                </androidx.appcompat.widget.LinearLayoutCompat>
            </androidx.appcompat.widget.LinearLayoutCompat>

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/button_filter"
                android:textAllCaps="false"
                android:textStyle="normal"
                android:textSize="@dimen/_11sdp"
                android:textColor="@color/white"
                android:fontFamily="@font/inter_medium"
                android:text="@string/search"
                android:gravity="center"
                android:background="@drawable/button_bac"
                android:layout_marginVertical="@dimen/_10sdp"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_30sdp"/>
        </androidx.appcompat.widget.LinearLayoutCompat>
    </androidx.core.widget.NestedScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>